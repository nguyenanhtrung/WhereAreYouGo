<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent">

  <TextView
    android:id="@+id/text_title_dialog"
    android:layout_width="0dp"
    android:layout_height="wrap_content"
    android:text="@string/title_profile_dialog"
    android:background="@color/colorPrimary"
    style="@style/TitleDialog"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintLeft_toLeftOf="parent" />

  <de.hdodenhof.circleimageview.CircleImageView
    android:id="@+id/circle_user_image"
    android:layout_width="@dimen/width_user_image"
    android:layout_height="@dimen/height_user_image"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginTop="8dp"
    app:layout_constraintTop_toBottomOf="@+id/text_title_dialog" />

  <TextView
    android:id="@+id/text_user_name"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="Cristiano Ronaldo"
    style="@style/TextUserName"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginTop="16dp"
    app:layout_constraintTop_toBottomOf="@+id/circle_user_image"
    />

  <de.hdodenhof.circleimageview.CircleImageView
    android:id="@+id/circle_email_image"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/ic_email"
    android:layout_marginTop="25dp"
    app:layout_constraintTop_toBottomOf="@+id/text_user_name"
    android:layout_marginStart="16dp"
    android:layout_marginLeft="16dp"
    app:layout_constraintLeft_toLeftOf="parent" />

  <TextView
    android:id="@+id/text_label_email"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/label_email"
    style="@style/TextLabel"
    android:layout_marginLeft="4dp"
    app:layout_constraintLeft_toRightOf="@+id/circle_email_image"
    android:layout_marginStart="4dp"
    android:layout_marginTop="34dp"
    app:layout_constraintTop_toBottomOf="@+id/text_user_name" />

  <TextView
    android:id="@+id/text_user_email"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="sinhtolocda2003@gmail.com"
    style="@style/TextView"
    app:layout_constraintLeft_toRightOf="@+id/text_label_email"
    android:layout_marginLeft="16dp"
    android:layout_marginStart="16dp"
    app:layout_constraintBaseline_toBaselineOf="@+id/text_label_email" />

  <de.hdodenhof.circleimageview.CircleImageView
    android:id="@+id/circle_phone_image"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/ic_phone_android_light_green_500_24dp"
    android:layout_marginTop="24dp"
    app:layout_constraintTop_toBottomOf="@+id/circle_email_image"
    android:layout_marginStart="20dp"
    android:layout_marginLeft="20dp"
    app:layout_constraintLeft_toLeftOf="parent" />

  <TextView
    android:id="@+id/text_label_phone"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/label_phone"
    android:layout_marginTop="30dp"
    app:layout_constraintTop_toBottomOf="@+id/divider_email"
    style="@style/TextLabel"
    android:layout_marginStart="8dp"
    app:layout_constraintLeft_toRightOf="@+id/circle_phone_image"
    android:layout_marginLeft="8dp" />

  <TextView
    android:id="@+id/text_user_phone"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="01663559312"
    style="@style/TextView"
    app:layout_constraintLeft_toRightOf="@+id/text_label_phone"
    android:layout_marginLeft="16dp"
    app:layout_constraintBaseline_toBaselineOf="@+id/text_label_phone"
    android:layout_marginStart="16dp" />

  <de.hdodenhof.circleimageview.CircleImageView
    android:id="@+id/image_address"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:src="@drawable/ic_home_light_green_500_24dp"
    android:layout_marginTop="30dp"
    app:layout_constraintTop_toBottomOf="@+id/divider_phone"
    android:layout_marginLeft="20dp"
    app:layout_constraintLeft_toLeftOf="parent"
    android:layout_marginStart="20dp" />

  <TextView
    android:id="@+id/text_label_address"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/label_address"
    style="@style/TextLabel"
    app:layout_constraintLeft_toRightOf="@+id/image_address"
    android:layout_marginLeft="8dp"
    android:layout_marginTop="36dp"
    app:layout_constraintTop_toBottomOf="@+id/divider_phone"
    android:layout_marginStart="8dp" />

  <TextView
    android:id="@+id/text_user_address"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="123 Phuc Tan"
    style="@style/TextView"
    app:layout_constraintLeft_toRightOf="@+id/text_label_address"
    android:layout_marginLeft="16dp"
    app:layout_constraintBaseline_toBaselineOf="@+id/text_label_address"
    android:layout_marginStart="16dp" />

  <TextView
    android:id="@+id/text_num_of_friends"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="123"
    android:textSize="32sp"
    android:gravity="center"
    android:textColor="@color/color_num_of_friend"
    android:layout_marginTop="68dp"
    app:layout_constraintTop_toBottomOf="@+id/image_address"
    android:layout_marginStart="32dp"
    android:layout_marginLeft="32dp"
    app:layout_constraintLeft_toLeftOf="parent" />

  <TextView
    android:id="@+id/label_num_of_friends"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/label_num_of_friends"
    style="@style/TextLabel"
    android:layout_marginTop="9dp"
    app:layout_constraintTop_toBottomOf="@+id/text_num_of_friends"
    android:layout_marginLeft="39dp"
    android:layout_marginStart="39dp"
    app:layout_constraintLeft_toLeftOf="parent" />

  <TextView
    android:id="@+id/label_num_of_follower"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/label_num_of_followers"
    style="@style/TextLabel"
    app:layout_constraintLeft_toRightOf="@+id/label_num_of_favorite_place"
    android:layout_marginLeft="28dp"
    android:layout_marginStart="28dp"
    app:layout_constraintBaseline_toBaselineOf="@+id/label_num_of_favorite_place" />

  <TextView
    android:id="@+id/text_num_of_follower"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="32sp"
    android:textColor="@color/color_num_of_follower"
    android:text="69"
    android:layout_marginRight="64dp"
    android:layout_marginEnd="64dp"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintBaseline_toBaselineOf="@+id/text_num_of_favorite_place" />

  <TextView
    android:id="@+id/text_num_of_favorite_place"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:textSize="32sp"
    android:textColor="@color/color_num_of_favor_venue"
    android:text="96"
    app:layout_constraintLeft_toRightOf="@+id/text_num_of_friends"
    android:layout_marginLeft="69dp"
    android:layout_marginStart="69dp"
    app:layout_constraintBaseline_toBaselineOf="@+id/text_num_of_friends" />

  <TextView
    android:id="@+id/label_num_of_favorite_place"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="@string/label_num_of_favorite_place"
    style="@style/TextLabel"
    app:layout_constraintLeft_toRightOf="@+id/label_num_of_friends"
    android:layout_marginLeft="31dp"
    app:layout_constraintBaseline_toBaselineOf="@+id/label_num_of_friends" />

  <View
    android:id="@+id/divider_one"
    android:layout_width="1dp"
    android:layout_height="60dp"
    android:background="@color/colorAccent"
    app:layout_constraintLeft_toRightOf="@+id/text_num_of_friends"
    android:layout_marginLeft="10dp"
    android:layout_marginStart="10dp"
    android:layout_marginTop="72dp"
    app:layout_constraintTop_toBottomOf="@+id/text_user_address" />

  <View
    android:id="@+id/divider_two"
    android:layout_width="1dp"
    android:layout_height="60dp"
    android:background="@color/colorAccent"
    app:layout_constraintLeft_toRightOf="@+id/text_num_of_favorite_place"
    android:layout_marginLeft="53dp"
    android:layout_marginStart="53dp"
    android:layout_marginTop="72dp"
    app:layout_constraintTop_toBottomOf="@+id/text_user_address" />

  <View
    android:id="@+id/divider_email"
    android:layout_width="0dp"
    android:layout_height="1dp"
    android:background="@color/colorAccent"
    app:layout_constraintTop_toBottomOf="@+id/text_user_email"
    android:layout_marginTop="8dp"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginLeft="16dp"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="16dp"
    app:layout_constraintHorizontal_bias="0.0" />

  <View
    android:id="@+id/divider_phone"
    android:layout_width="0dp"
    android:layout_height="1dp"
    android:background="@color/colorAccent"
    app:layout_constraintTop_toBottomOf="@+id/text_user_phone"
    android:layout_marginTop="8dp"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginLeft="16dp"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="16dp"
    app:layout_constraintHorizontal_bias="0.0" />

  <View
    android:id="@+id/divider_address"
    android:layout_width="0dp"
    android:layout_height="1dp"
    android:background="@color/colorAccent"
    app:layout_constraintTop_toBottomOf="@+id/text_user_address"
    android:layout_marginTop="8dp"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    android:layout_marginLeft="16dp"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp"
    android:layout_marginRight="16dp"
    app:layout_constraintHorizontal_bias="0.0" />



</android.support.constraint.ConstraintLayout>